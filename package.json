    },
    "dependencies": {
        "@babel/runtime": "^7.3.1",
        "@rgrove/parse-xml": "^1.1.1",
        "browserify": "^16.2.3",
        "chalk": "^2.4.2",
        "fs-extra": "^7.0.1",
        "he": "^1.2.0",
        "iconv-lite": "^0.4.24",
        "jsonpath": "^1.0.0",
        "papaparse": "^4.6.3",
        "perl-regex": "^1.0.4",
        "prettier": "^1.16.1",
        "yaml": "^1.3.2",
        "yargs": "^12.0.5"
    },
    "devDependencies": {
        "ava": "^1.0.0-rc.2",
        "cross-env": "^5.2.0",
        "decache": "^4.5.0",
        "jsdoc": "^3.5.5",
        "mock-require": "^3.0.2",
        "npm-run-all": "^4.1.5",
        "sinon": "^7.1.1",
        "snazzy": "^8.0.0",
        "standard": "^12.0.1"
    },
    "scripts": {
        "lint-js": "standard",
        "lint": "npm-run-all lint-js",
        "doc": "jsdoc -c .jsdoc.json --pedantic",
        "test-int": "cross-env NODE_PATH=src:test ava test/int",
        "test-unit": "cross-env NODE_PATH=src:test ava test/unit",
        "test": "npm-run-all test-unit test-int",
        "test-hoc": "cross-env NODE_PATH=src:test ava",

{
    "opts": {
        "destination": "api",
        "encoding": "utf8",
        "recurse": true
    },
    "tags": {
        "allowUnknownTags": false
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "default": {
            "outputSourceFiles": false
        }
    },
    "source": {
        "include": [
            "src"
        ]
    }
}
